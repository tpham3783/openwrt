config wifi-device  radio0
	option type     mac80211
	option channel  11
	option hwmode	11g
	option path	'10180000.wmac'
	option htmode	HT20
	# REMOVE THIS LINE TO ENABLE WIFI:
	option disabled 1

config wifi-iface
	option device   radio0
	option network  lan
	option mode     ap
	option ssid     AP102B-Radio0
	option encryption none

